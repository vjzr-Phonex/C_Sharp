"code" class=3D"html" style=3D"display: none;">&lt;?xml version=3D"1.0"?&gt=
;
&lt;configuration&gt;
  &lt;configSections&gt;
    &lt;section name=3D"log4net"=20
             type=3D"log4net.Config.Log4NetConfigurationSectionHandler,log4=
net"/&gt;
  &lt;/configSections&gt;
  &lt;log4net&gt;
    &lt;logger name=3D"DefaultLogger"&gt;
      &lt;level value=3D"ALL"/&gt;
      &lt;appender-ref ref=3D"InfoLoging" /&gt;
      &lt;appender-ref ref=3D"ErrorLoging" /&gt;
    &lt;/logger&gt;
    &lt;!--DefaultLogger Info Log--&gt;
    &lt;appender name=3D"InfoLoging"=20
              type=3D"log4net.Appender.RollingFileAppender,log4net" &gt;
      &lt;File value=3D"Log\LogTipMsg.txt" /&gt;
      &lt;datePattern value=3D"(yyyyMMdd)"/&gt;
      &lt;appendToFile value=3D"true"/&gt;
      &lt;RollingStyle value=3D"Composite"/&gt;
      &lt;MaxSizeRollBackups value=3D"10"/&gt;
      &lt;maximumFileSize value=3D"1MB"/&gt;
      &lt;layout type=3D"log4net.Layout.PatternLayout"&gt;
        &lt;conversionPattern value=3D"%date [%t]%-5p %c - %m%n"/&gt;
      &lt;/layout&gt;
      &lt;filter type=3D"log4net.Filter.LevelRangeFilter"&gt;
        &lt;LevelMin value=3D"DEBUG"/&gt;
        &lt;LevelMax value=3D"Warn"/&gt;
      &lt;/filter&gt;
    &lt;/appender&gt;
    &lt;!--DefaultLogger Error Log--&gt;
    &lt;appender name=3D"ErrorLoging"=20
